#call git
git

#second
git init

#inisialisasi dari source gitlab/github
git remote add origin https://gitlab.com/andrihadiansyah1122/android.git

#menambahkan updatetan
git add .

#untuk commit masuk ke gitlab email & name
#check pertama
git commit -am "declare"

git config --global user.email "andrihadiansyah1122@gmail.com"
git config --global user.name "andrihadiansyah1122"

#ngecek file sebelum di reupload
git status

#check kedua
git commit -am "declare"

#ingin menambahkan
git add .

#3
git push origin master



#ambil updaten
git pull origin master
